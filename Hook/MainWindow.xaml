<Window x:Class="Hook.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hook"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<Grid Loaded="Grid_Loaded">
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="440*"/>
			<ColumnDefinition Width="91*"/>
			<ColumnDefinition Width="261*"/>
		</Grid.ColumnDefinitions>
		<TextBox x:Name="showResult" Grid.ColumnSpan="3" HorizontalAlignment="Left" Height="230" Margin="41,80,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="682" FontSize="16" IsReadOnly="True" VerticalScrollBarVisibility="Auto"/>

	</Grid>
</Window>
